{
 "name": "@certitrade/card2-model",
 "version": "2.0.15",
 "description": "Javascript ibrary containing the model for the Certitrade Card 2 REST API.",
 "author": "Simon Mika <simon.mika@certitrade.se>",
 "license": "MIT",
 "repository": {
  "type": "git",
  "url": "git+https://github.com/certitrade/card2-model"
 },
 "bugs": {
  "url": "https://github.com/certitrade/card2-model/issues"
 },
 "homepage": "https://github.com/certitrade/card2-model#readme",
 "private": false,
 "main": "dist",
 "typings": "dist",
 "jest": {
  "globals": {
   "ts-jest": {
    "tsConfig": "tsconfig.test.json"
   }
  },
  "testEnvironment": "node",
  "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
  "testPathIgnorePatterns": [
   "node_modules/",
   "dist/"
  ],
  "collectCoverageFrom": [
   "**/*.{ts,tsx,js,jsx}",
   "!**/node_modules/**",
   "!**/dist*"
  ],
  "preset": "ts-jest",
  "testMatch": null
 },
 "scripts": {
  "dev": "watch 'npm run build' .",
  "lint": "tslint --config tslint.json --project .",
  "build": "tsc -p .",
  "test": "jest",
  "test:watch": "watch jest",
  "prepare": "npm run build",
  "clean": "rm -rf dist node_modules coverage"
 },
 "git": {
  "tagName": "v${version}"
 },
 "github": {
  "release": true
 },
 "dependencies": {
  "@certitrade/card2-model": "^2.0.6",
  "isoly": "0.0.6",
  "node-fetch": "^2.3.0"
 },
 "devDependencies": {
  "@types/jest": "^24.0.11",
  "@types/node": "^11.11.6",
  "@types/node-fetch": "^2.1.7",
  "babel-core": "^6.26.3",
  "babel-jest": "^24.5.0",
  "jest": "^24",
  "ts-jest": "^24.0.0",
  "tslint": "^5.14.0",
  "typescript": "^3.3.4000",
  "watch": "^1.0.2"
 }
}
